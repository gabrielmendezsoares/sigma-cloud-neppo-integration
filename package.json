{
  "name": "sigma-cloud-neppo-integration",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start:development": "cross-env NODE_ENV=development nodemon",
    "start:production": "cross-env NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist node_modules/.tmp/tsconfig.tsbuildinfo",
    "build": "tsup src/index.ts --format esm --minify --clean --sourcemap",
    "build:watch": "tsup src/index.ts --format esm --watch",
    "db:pull": "node scripts/runCommandOnAllSchemas.script.js db:pull",
    "db:push": "node scripts/runCommandOnAllSchemas.script.js db:push",
    "db:generate": "node scripts/runCommandOnAllSchemas.script.js db:generate",
    "db:migrate:dev": "node scripts/runCommandOnAllSchemas.script.js db:migrate:dev",
    "db:migrate:deploy": "node scripts/runCommandOnAllSchemas.script.js db:migrate:deploy",
    "db:studio": "node scripts/runCommandOnAllSchemas.script.js db:studio",
    "db:reset": "npm run db:pull && npm run db:generate",
    "docker:build:development": "docker build --target development -t sigma-cloud-neppo-integration:development .",
    "docker:build:production": "docker build --target production -t sigma-cloud-neppo-integration:production .",
    "docker:run:development": "docker run --user root -p 3928:3928 --env-file .env sigma-cloud-neppo-integration:development",
    "docker:run:production": "docker run --user root -p 3928:3928 --env-file .env sigma-cloud-neppo-integration:production",
    "docker:compose:up:development": "docker-compose up main-development -d",
    "docker:compose:up:production": "docker-compose up main-production -d",
    "docker:compose:up:build:development": "docker-compose up main-development -d --build",
    "docker:compose:up:build:production": "docker-compose up main-production -d --build",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "docker:prune": "docker system prune -af",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "npm run db:reset"
  },
  "engines": {
    "node": ">=20.14.0"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.0",
    "node-os-utils": "^1.3.7",
    "node-schedule": "^2.1.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-validator": "^2.20.33",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/moment": "^2.11.29",
    "@types/moment-timezone": "^0.5.13",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/node-os-utils": "^1.3.4",
    "@types/node-schedule": "^2.1.7",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.1.9",
    "prisma": "^6.6.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
